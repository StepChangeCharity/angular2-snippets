{
  "name": "angular2-seed",
  "version": "1.0.0",
  "description": "Starting point for a blank ng2 application",
  "main": "app/bootstrap",
  "scripts": {
    "development": "gulp development",
    "production": "gulp production",
    "build": "gulp build",
    "setup": "npm install -g live-server tsd jspm && npm install && jspm install & tsd reinstall -s -o"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChrisMurphy/angular2-seed.git"
  },
  "author": "Chris Murphy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ChrisMurphy/angular2-seed/issues"
  },
  "keywords": [
    "web",
    "frontend",
    "html5",
    "angularjs",
    "angular2",
    "typescript",
    "es6"
  ],
  "homepage": "https://github.com/ChrisMurphy/angular2-seed",
  "devDependencies": {
    "del": "^2.0.2",
    "gulp": "^3.9.0",
    "gulp-environments": "^0.1.1",
    "gulp-html-replace": "^1.5.4",
    "gulp-htmlmin": "^1.1.4",
    "gulp-sequence": "^0.4.1",
    "gulp-tslint": "^3.3.0",
    "gulp-useref": "^1.3.0",
    "jspm": "^0.16.10",
    "live-server": "^0.8.1",
    "require-dir": "^0.3.0"
  },
  "jspm": {
    "directories": {
      "baseURL": "src"
    },
    "dependencies": {
      "@reactivex/rxjs": "npm:@reactivex/rxjs@^5.0.0-alpha.2",
      "angular2": "npm:angular2@^2.0.0-alpha.39",
      "es6-shim": "github:es-shims/es6-shim@^0.33.6",
      "reflect-metadata": "npm:reflect-metadata@^0.1.2",
      "zone.js": "npm:zone.js@^0.5.6"
    },
    "devDependencies": {
      "typescript": "npm:typescript@^1.6.2"
    },
    "overrides": {
      "npm:@reactivex/rxjs@5.0.0-alpha.2": {
        "format": "cjs"
      }
    }
  }
}
